{
  "name": "polar-desktop-app",
  "version": "2.1.44",
  "description": "Polar",
  "main": "./main-bundle.js",
  "private": true,
  "scripts": {
    "start": "electron .",
    "dist-linux": "npx webpack && electron-builder --config=electron-builder.yml --linux appimage deb tar.gz --publish always",
    "dist-macos": "npx webpack && electron-builder --config=electron-builder.yml --macos dmg zip --publish always",
    "dist-win": "npx webpack && ./build/dist-win.sh",
    "verify-electron-version": "./scripts/verify-electron-version"
  },
  "repository": "https://github.com/burtonator/polar-app-public",
  "keywords": [
    "Electron",
    "PDF",
    "viewer",
    "PDF.js",
    "ebook",
    "annotations"
  ],
  "author": {
    "name": "contributors",
    "email": "noone@example.com"
  },
  "contributors": [
    {
      "name": "Kevin Burton"
    }
  ],
  "build": {
    "appId": "io.inputneuron/polar",
    "mac": {
      "category": "Books"
    }
  },
  "license": "GPL-3.0-only",
  "devDependencies": {
    "cache-loader": "4.1.0",
    "electron": "=12.0.2",
    "electron-builder": "=22.10.5",
    "electron-notarize": "1.0.0",
    "electron-updater": "=4.3.8",
    "fork-ts-checker-webpack-plugin": "=6.2.0",
    "mocha": "=8.2.1",
    "null-loader": "=4.0.1",
    "source-map-support": "=0.5.19",
    "thread-loader": "=3.0.1",
    "ts-loader": "=8.0.18",
    "tslint": "6.1.2",
    "typescript": "=4.2.3",
    "webpack": "=5.26.3",
    "webpack-cli": "=4.5.0"
  },
  "dependencies": {
    "polar-bookshelf": "^2.1.44",
    "polar-shared": "^2.1.44"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/polar-bookshelf"
  }
}
